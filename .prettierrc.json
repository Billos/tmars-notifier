{
  "semi": false,
  "singleQuote": false,
  "printWidth": 140,
  "trailingComma": "all",
  "plugins": [
    "prettier-plugin-packagejson",
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-organize-imports",
    "prettier-plugin-multiline-arrays"
  ],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^[.]"
  ],
  "multilineArraysWrapThreshold": 5,
  "objectWrap": "collapse"
}
